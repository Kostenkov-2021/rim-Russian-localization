msgstr "Правка"

msgid "Speech"
msgstr "Речь"

msgid "Window"
msgstr "Окно"

msgid "RIM Manual"
msgstr "Руководство RIM"

msgid "This computer is now ready for remote access."
msgstr "Теперь этот компьютер готов к удалённому доступу."

msgid "Version"
msgstr "Версия"

msgid "Support and Feedback"
msgstr "Поддержка и обратная связь"

msgid "Read the Manual"
msgstr "Прочитать руководство пользователя"

msgid "View Change History"
msgstr "Просмотреть историю изменений"

msgid "Send Logs"
